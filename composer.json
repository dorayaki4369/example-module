{
    "name": "dorayaki4369/example-module",
    "type": "laravel-module",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryuya Hirayama",
            "email": "ryuya.hirayama@fourier.jp"
        }
    ],
    "require": {
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Modules\\Example\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Example\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
